<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/HibernateMappingDTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="circle.po.Dynamic" table="dynamic" catalog="class_circle">
		<id name="dynamicID" type="long">
			<column name="dynamicID" />
			<generator class="increment" />
		</id>
		<property name="userID" type="java.lang.Long">
			<column name="userID"/>
		</property>
		<property name="time" type="date">
			<column name="time" />
		</property>
		<property name="content" type="string">
			<column name="content" />
		</property>
		<property name="imageUrls" type="string">
			<column name="imageUrls" />
		</property>
		<property name="likeAmount" type="integer">
			<column name="likeAmount" />
		</property>
	</class>
</hibernate-mapping>